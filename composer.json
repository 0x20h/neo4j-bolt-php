{
    "name": "graphaware/neo4j-bolt",
    "description": "Neo4j Bolt Binary Protocol PHP Driver",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Willemsen",
            "email": "christophe@graphaware.com"
        }
    ],
    "require": {
        "symfony/event-dispatcher": "^2.7",
        "monolog/monolog": "^1.16",
        "doctrine/collections": "^1.3",
        "symfony/console": "^2.7"
    },
    "autoload": {
      "psr-4": {
          "GraphAware\\Bolt\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "GraphAware\\Bolt\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "neoxygen/neoclient": "^3.3",
        "symfony/stopwatch": "^2.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
